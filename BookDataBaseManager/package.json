{
  "name": "BookDataBaseManager",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "main.js",
  "description": "Vue + Electron desktop app for bookstore's databse management",
  "author": "Kevin",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "echarts": "^5.6.0",
    "font-awesome": "^4.7.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "concurrently": "^9.2.0",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "wait-on": "^8.0.4"
  }
}
